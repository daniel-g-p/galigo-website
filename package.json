{
    "name": "galigo",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "watch-sass": "sass --watch scss:public/styles --style compressed",
        "compile-css": "sass scss/main.scss public/styles/comp.css",
        "prefix-css": "postcss public/styles/comp.css --use autoprefixer -o public/styles/prefix.css",
        "compress-css": "sass public/styles/prefix.css public/styles/main.css --style compressed",
        "build-css": "npm-run-all compile-css prefix-css compress-css"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/daniel-g-p/galigo-website.git"
    },
